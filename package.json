{
  "name": "gatsby-quickstart",
  "description": "Starter template for a static site built with Gatsby",
  "version": "0.2.0",
  "author": "Michael Currin",
  "license": "0BSD",
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "echo 'Write tests! -> https://gatsby.dev/unit-testing' && exit 1",
    "preversion": "npm run format",
    "version": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "gatsby": "^2.24.85",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelCurrin/gatsby-quickstart"
  },
  "bugs": {
    "url": "https://github.com/MichaelCurrin/gatsby-quickstart"
  }
}
